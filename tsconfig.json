    "compilerOptions": {
        "target": "ES5",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitUseStrict": true,
        "noImplicitReturns": true,
        "typeRoots" : ["./node_modules/@types/"]
    },
    "exclude": [
        "lassy-xpath",
        "web-ui"
    ]
}